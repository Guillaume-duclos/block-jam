{
  "expo": {
    "name": "block-jam",
    "slug": "block-jam",
    "version": "1.0.0",
    "userInterfaceStyle": "automatic",
    "android": {
      "package": "com.anonymous.blockjam"
    },
    "ios": {
      "bundleIdentifier": "com.anonymous.blockjam"
    },
    "plugins": [
      "expo-localization",
      [
        "expo-font",
        {
          "fonts": [
            "./app/assets/fonts/Rubik/RubikItalicVariable.ttf",
            "./app/assets/fonts/Rubik/RubikVariable.ttf",
            "./app/assets/fonts/GoogleSansCode/GoogleSansCodeVariable.ttf",
            "./app/assets/fonts/GoogleSansCode/GoogleSansCodeItalicVariable.ttf"
          ]
        }
      ]
    ]
  }
}
